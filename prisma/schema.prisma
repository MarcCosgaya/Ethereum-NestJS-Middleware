// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model PKey {
  value String @unique
}

model contract {
  id Int @id @default(autoincrement())
  abi String
  bytecode String
  source String
  address String @unique
}

model transaction {
  id Int @id @default(autoincrement())
  from String
  to String
  quantity BigInt
  hash String @unique
  blockHeight Int?
  gasUsed BigInt?
  gasPrice BigInt?
  gasLimit BigInt
}